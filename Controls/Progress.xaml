<UserControl x:Class="Telesyk.SecuredSource.UI.Controls.ProgressControl"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
	xmlns:local="clr-namespace:Telesyk.SecuredSource.UI.Controls"
	mc:Ignorable="d" 
	d:DesignHeight="450" d:DesignWidth="800">
	<Grid x:Name="GridProgress" Style="{StaticResource ProgressGrid}">
		<Grid.ColumnDefinitions>
			<ColumnDefinition />
			<ColumnDefinition Width="32" />
		</Grid.ColumnDefinitions>
		<ProgressBar Grid.Column="0" x:Name="Progress" />
		<TextBlock Grid.Column="1"><Run x:Name="TextPercentage" Text="0" />%</TextBlock>
	</Grid>
</UserControl>
