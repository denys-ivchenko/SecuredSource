<UserControl x:Class="Telesyk.SecuredSource.UI.Controls.DESPanelControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Telesyk.SecuredSource.UI.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="320">
	<UserControl.Resources>
	</UserControl.Resources>
	<StackPanel>
		<TextBlock Style="{StaticResource PanelFieldHeader}">Control 1:</TextBlock>
		<RadioButton Style="{StaticResource PanelFieldControl}">Value 1</RadioButton>
		<TextBlock Style="{StaticResource PanelFieldHeader}">Control 2:</TextBlock>
		<RadioButton Style="{StaticResource PanelFieldControl}">Value 2</RadioButton>
		<TextBlock Style="{StaticResource PanelFieldHeader}">Control 3:</TextBlock>
		<RadioButton Style="{StaticResource PanelFieldControl}">Value 3</RadioButton>
		<TextBlock Style="{StaticResource PanelFieldHeader}">Control 4:</TextBlock>
		<RadioButton Style="{StaticResource PanelFieldControl}">Value 4</RadioButton>
		<TextBlock Style="{StaticResource PanelFieldHeader}">Control 5:</TextBlock>
		<RadioButton Style="{StaticResource PanelFieldControl}">Value 5</RadioButton>
	</StackPanel>
</UserControl>
