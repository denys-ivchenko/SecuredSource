<UserControl x:Class="Telesyk.SecuredSource.UI.Controls.SelectAlgorithmControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:local="clr-namespace:Telesyk.SecuredSource.UI.Controls"
	xmlns:res="clr-namespace:Telesyk.SecuredSource.Globalization;assembly=Telesyk.SecuredSource.Localization"
    mc:Ignorable="d" 
    d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
		<TextBlock Style="{StaticResource PanelFieldHeader}"><Run Text="{x:Static res:Strings.Algorithm}" />:</TextBlock>
		<ComboBox x:Name="SelectAlgorithm" SelectionChanged="SelectAlgorythm_SelectionChanged" />
	</StackPanel>
</UserControl>
